APP_NAME="RS Puri Asih API"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8001

LOG_CHANNEL=stack
LOG_LEVEL=debug

# Database - Gunakan database dari parent directory
DB_CONNECTION=sqlite
DB_DATABASE=../database/database.sqlite

# CORS - Allow origins (comma-separated)
CORS_ALLOWED_ORIGINS=http://localhost:8000,http://localhost:3000

# Cache & Session
CACHE_DRIVER=file
QUEUE_CONNECTION=sync
SESSION_DRIVER=file

# API Rate Limiting
API_RATE_LIMIT=60
